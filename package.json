{
  "name": "anneleenvernaillen.com",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "deploy": "nuxt build && netlify deploy --dir dist",
    "serve": "pnpm run generate && serve dist",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "cypress:install": "cypress install",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:dev": "start-server-and-test dev http://localhost:3000 'cypress open --config baseUrl=http://localhost:3000'",
    "cypress:local": "start-server-and-test serve http://localhost:3000 cypress:run --config baseUrl=http://localhost:3000",
    "lint": "eslint",
    "lint:fix": "eslint . --fix",
    "fulltest": "rm -rf node_modules && pnpm i && pnpm run lint && pnpm run coverage && pnpm run cypress:local"
  },
  "eslintConfig": {
    "extends": "@antfu"
  },
  "dependencies": {
    "animate.css": "^4.1.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.33.1",
    "@nuxt/content": "^2.2.2",
    "@nuxt/image-edge": "1.0.0-27827328.bc9ddc0",
    "@nuxt/test-utils-edge": "3.0.1-rc.0-27837437.9c478ee",
    "@nuxtjs/google-fonts": "^3.0.0-1",
    "@nuxtjs/i18n": "8.0.0-beta.6",
    "@nuxtjs/tailwindcss": "^6.1.3",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/container-queries": "^0.1.0",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.8",
    "@vitest/coverage-c8": "^0.25.4",
    "cypress": "^11.2.0",
    "dayjs": "^1.11.6",
    "jsdom": "^20.0.3",
    "nuxt": "^3.0.0",
    "nuxt-icon": "^0.1.8",
    "remark-directive-rehype": "^0.4.2",
    "sitemap": "^7.1.1",
    "start-server-and-test": "^1.14.0",
    "vitest": "^0.25.4"
  }
}
