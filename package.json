{
  "name": "anneleenvernaillen.com",
  "type": "module",
  "version": "0.8.0",
  "private": false,
  "author": "Wouter Vernaillen",
  "main": "./nuxt.config.ts",
  "scripts": {
    "dev": "nuxi prepare & nuxi dev",
    "build": "nuxi build",
    "serve": "nuxi build && node .output/server/index.mjs",
    "update": "git pull && pnpm update -L",
    "release": "release-it patch --ci",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest --",
    "coverage": "vitest run --coverage",
    "clean": "pnpm recursive exec -- rm -rf node_modules; rm -rf node_modules",
    "fulltest": "pnpm run clean && pnpm config set auto-install-peers true && pnpm install --shamefully-hoist=true && pnpm run lint && pnpm run coverage && pnpm run build"
  },
  "dependencies": {
    "@nuxt/content": "^2.13.1",
    "@nuxt/image": "^1.7.0",
    "@nuxt/kit": "^3.12.3",
    "@nuxt/ui": "^2.17.0",
    "@vernaillen/nuxt-base-layer": "0.3.10",
    "vue-gtag": "^2.0.1"
  },
  "devDependencies": {
    "@iconify-json/simple-icons": "^1.1.109",
    "@nuxt/fonts": "^0.7.1",
    "@nuxt/test-utils": "^3.13.1",
    "@nuxthq/studio": "^2.0.3",
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@nuxtjs/i18n": "^8.3.1",
    "@nuxtjs/plausible": "^1.0.0",
    "@pinia/nuxt": "^0.5.1",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.13",
    "@vitest/coverage-v8": "^2.0.3",
    "@vue/test-utils": "^2.4.6",
    "@vueuse/core": "^10.11.0",
    "@vueuse/nuxt": "^10.11.0",
    "animate.css": "^4.1.1",
    "consola": "^3.2.3",
    "cypress": "^13.13.0",
    "dayjs": "^1.11.11",
    "eslint": "^9.7.0",
    "happy-dom": "^14.12.3",
    "nuxt": "^3.12.3",
    "nuxt-config-schema": "^0.4.6",
    "pinia": "^2.1.7",
    "playwright-core": "^1.45.1",
    "remark-directive-rehype": "^0.4.2",
    "sitemap": "^8.0.0",
    "start-server-and-test": "^2.0.4",
    "typescript": "^5.5.3",
    "vee-validate": "^4.13.2",
    "vitest": "2.0.3",
    "vitest-environment-nuxt": "1.0.0",
    "vue-easy-lightbox": "^1.19.0",
    "yup": "^1.4.0"
  },
  "engines": {
    "node": ">=18",
    "pnpm": ">=9.3.0"
  }
}
