{
  "name": "app",
  "version": "0.5.2",
  "private": false,
  "type": "module",
  "main": "./nuxt.config.ts",
  "scripts": {
    "dev": "nuxi prepare & nuxi dev --open",
    "build": "nuxi build",
    "serve": "nuxi build && node .output/server/index.mjs",
    "update": "git pull && pnpm update -L",
    "release": "release-it patch --ci",
    "lint": "pnpm recursive run lint",
    "test": "pnpm recursive run test",
    "clean": "pnpm recursive exec -- rm -rf node_modules; rm -rf node_modules",
    "generateAdminCss": "tailwindcss -i ./assets/css/tailwind.css -o ./cms/public/css/tailwind.css && tailwindcss -i ./assets/css/prose.css -o ./cms/public/css/prose.css && tailwindcss -i ./assets/css/markdown.css -o ./cms/public/css/markdown.css",
    "fulltest": "pnpm run clean && pnpm config set auto-install-peers true && pnpm install --shamefully-hoist=true && pnpm run lint && pnpm run test && pnpm run build && pnpm run dev"
  },
  "dependencies": {
    "@nuxt/content": "^2.4.3",
    "@nuxt/image-edge": "1.0.0-27919678.2f5b64b",
    "@nuxt/kit": "^3.2.2",
    "@nuxt/ui-templates": "^1.1.1",
    "nuxt": "^3.2.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.35.2",
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@nuxtjs/google-fonts": "3.0.0-1",
    "@nuxtjs/i18n": "8.0.0-beta.9",
    "@nuxtjs/tailwindcss": "^6.4.1",
    "@pinia/nuxt": "^0.4.6",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/container-queries": "^0.1.0",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.9",
    "@vitest/coverage-c8": "^0.28.5",
    "@vueuse/core": "^9.13.0",
    "@vueuse/nuxt": "^9.13.0",
    "animate.css": "^4.1.1",
    "cypress": "^12.6.0",
    "dayjs": "^1.11.7",
    "eslint": "^8.34.0",
    "nuxt-config-schema": "^0.4.4",
    "nuxt-icon": "^0.2.11",
    "pinia": "^2.0.30",
    "release-it": "^15.6.0",
    "remark-directive-rehype": "^0.4.2",
    "start-server-and-test": "^1.15.4",
    "typescript": "^4.9.5",
    "vee-validate": "^4.7.4",
    "vitest": "^0.28.5",
    "vue-easy-lightbox": "^1.12.0",
    "yup": "^1.0.0"
  },
  "eslintConfig": {
    "extends": "@antfu"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "axios": "0.25.0"
      },
      "ignoreMissing": [
        "postcss",
        "postcss-*",
        "webpack",
        "axios"
      ]
    }
  },
  "release-it": {
    "git": {
      "commit": true,
      "tag": true,
      "push": true,
      "commitMessage": "chore: release v${version}"
    },
    "github": {
      "release": false
    },
    "npm": {
      "publish": false
    }
  }
}
