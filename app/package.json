{
  "name": "app",
  "type": "module",
  "version": "0.7.0",
  "private": false,
  "main": "./nuxt.config.ts",
  "scripts": {
    "dev": "nuxi prepare & nuxi dev --open",
    "build": "nuxi build",
    "serve": "nuxi build && node .output/server/index.mjs",
    "update": "git pull && pnpm update -L",
    "release": "release-it patch --ci",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest --",
    "coverage": "vitest run --coverage",
    "clean": "pnpm recursive exec -- rm -rf node_modules; rm -rf node_modules",
    "fulltest": "pnpm run clean && pnpm config set auto-install-peers true && pnpm install --shamefully-hoist=true && pnpm run lint && pnpm run coverage && pnpm run build"
  },
  "dependencies": {
    "@nuxt/content": "^2.9.0",
    "@nuxt/image": "1.0.0",
    "@nuxt/kit": "^3.8.1",
    "@nuxt/ui-templates": "^1.3.1",
    "@tato30/vue-pdf": "^1.7.4",
    "@vernaillen/nuxt-base-layer": "0.2.21",
    "nuxt": "^3.8.1",
    "vue-gtag": "^2.0.1"
  },
  "devDependencies": {
    "@nuxthq/studio": "^1.0.4",
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@nuxtjs/google-fonts": "3.0.2",
    "@nuxtjs/i18n": "^8.0.0-rc.4",
    "@nuxtjs/plausible": "^0.2.3",
    "@nuxtjs/tailwindcss": "^6.9.4",
    "@pinia/nuxt": "^0.5.1",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@vitest/coverage-c8": "^0.33.0",
    "@vitest/coverage-v8": "^0.34.6",
    "@vueuse/core": "^10.6.0",
    "@vueuse/nuxt": "^10.6.0",
    "animate.css": "^4.1.1",
    "consola": "^3.2.3",
    "cypress": "^13.5.0",
    "dayjs": "^1.11.10",
    "eslint": "^8.53.0",
    "happy-dom": "^12.10.3",
    "nuxt-config-schema": "^0.4.6",
    "nuxt-icon": "^0.6.5",
    "nuxt-vitest": "^0.11.3",
    "pinia": "^2.1.7",
    "remark-directive-rehype": "^0.4.2",
    "sitemap": "^7.1.1",
    "start-server-and-test": "^2.0.2",
    "typescript": "^5.2.2",
    "vee-validate": "^4.11.8",
    "vitest": "0.33.0",
    "vitest-environment-nuxt": "0.11.3",
    "vue-easy-lightbox": "^1.16.0",
    "yup": "^1.3.2"
  }
}
