{
  "name": "app",
  "type": "module",
  "version": "0.6.3",
  "private": false,
  "main": "./nuxt.config.ts",
  "scripts": {
    "dev": "nuxi prepare & nuxi dev --open",
    "build": "nuxi build",
    "serve": "nuxi build && node .output/server/index.mjs",
    "update": "git pull && pnpm update -L",
    "release": "release-it patch --ci",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest --",
    "coverage": "vitest run --coverage",
    "clean": "pnpm recursive exec -- rm -rf node_modules; rm -rf node_modules",
    "fulltest": "pnpm run clean && pnpm config set auto-install-peers true && pnpm install --shamefully-hoist=true && pnpm run lint && pnpm run coverage && pnpm run build"
  },
  "dependencies": {
    "@nuxt/content": "^2.6.0",
    "@nuxt/image-edge": "1.0.0-28050764.aaee7ac",
    "@nuxt/kit": "^3.4.3",
    "@nuxt/ui-templates": "^1.1.1",
    "@vernaillen/nuxt-base-layer": "0.1.26",
    "nuxt": "^3.4.3",
    "vue-gtag": "^2.0.1"
  },
  "devDependencies": {
    "@nuxt/test-utils": "^3.4.3",
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@nuxtjs/google-fonts": "3.0.0",
    "@nuxtjs/i18n": "8.0.0-beta.9",
    "@nuxtjs/plausible": "^0.2.0",
    "@nuxtjs/tailwindcss": "^6.6.7",
    "@pinia/nuxt": "^0.4.9",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "@vitest/coverage-c8": "^0.31.0",
    "@vueuse/core": "^10.1.2",
    "@vueuse/nuxt": "^10.1.2",
    "animate.css": "^4.1.1",
    "consola": "^3.1.0",
    "cypress": "^12.11.0",
    "dayjs": "^1.11.7",
    "eslint": "^8.40.0",
    "nuxt-config-schema": "^0.4.6",
    "nuxt-icon": "^0.4.0",
    "pinia": "^2.0.35",
    "release-it": "^15.10.3",
    "remark-directive-rehype": "^0.4.2",
    "sitemap": "^7.1.1",
    "start-server-and-test": "^2.0.0",
    "typescript": "^5.0.4",
    "vee-validate": "^4.8.6",
    "vitest": "^0.31.0",
    "vue-easy-lightbox": "^1.16.0",
    "yup": "^1.1.1"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "axios": "0.25.0"
      },
      "ignoreMissing": [
        "postcss",
        "postcss-*",
        "webpack",
        "axios"
      ]
    }
  },
  "release-it": {
    "git": {
      "commit": true,
      "tag": true,
      "push": true,
      "commitMessage": "chore: release v${version}"
    },
    "github": {
      "release": false
    },
    "npm": {
      "publish": false
    }
  },
  "packageManager": "pnpm@8.4.0"
}
