{
  "name": "app",
  "type": "module",
  "version": "0.5.4",
  "private": false,
  "main": "./nuxt.config.ts",
  "scripts": {
    "dev": "nuxi prepare & nuxi dev --open",
    "build": "nuxi build",
    "serve": "nuxi build && node .output/server/index.mjs",
    "update": "git pull && pnpm update -L",
    "release": "release-it patch --ci",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest --",
    "coverage": "vitest run --coverage",
    "clean": "pnpm recursive exec -- rm -rf node_modules; rm -rf node_modules",
    "generateAdminCss": "tailwindcss -i ./assets/css/tailwind.css -o ./cms/public/css/tailwind.css && tailwindcss -i ./assets/css/prose.css -o ./cms/public/css/prose.css && tailwindcss -i ./assets/css/markdown.css -o ./cms/public/css/markdown.css",
    "fulltest": "pnpm run clean && pnpm config set auto-install-peers true && pnpm install --shamefully-hoist=true && pnpm run lint && pnpm run test && pnpm run build && pnpm run dev"
  },
  "dependencies": {
    "@nuxt/content": "^2.5.2",
    "@nuxt/image-edge": "1.0.0-28020728.5df24eb",
    "@nuxt/kit": "^3.4.1",
    "@nuxt/ui-templates": "^1.1.1",
    "nuxt": "^3.4.1",
    "vue-gtag": "^2.0.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.38.4",
    "@nuxt/test-utils": "^3.4.1",
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@nuxtjs/google-fonts": "3.0.0",
    "@nuxtjs/i18n": "8.0.0-beta.9",
    "@nuxtjs/plausible": "^0.2.0",
    "@nuxtjs/tailwindcss": "^6.6.6",
    "@pinia/nuxt": "^0.4.8",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "@vitest/coverage-c8": "^0.30.1",
    "@vueuse/core": "^10.0.2",
    "@vueuse/nuxt": "^10.0.2",
    "animate.css": "^4.1.1",
    "consola": "^3.0.2",
    "cypress": "^12.9.0",
    "dayjs": "^1.11.7",
    "eslint": "^8.38.0",
    "nuxt-config-schema": "^0.4.5",
    "nuxt-icon": "^0.3.3",
    "pinia": "^2.0.34",
    "release-it": "^15.10.1",
    "remark-directive-rehype": "^0.4.2",
    "sitemap": "^7.1.1",
    "start-server-and-test": "^2.0.0",
    "typescript": "^5.0.4",
    "vee-validate": "^4.8.5",
    "vitest": "^0.30.1",
    "vue-easy-lightbox": "^1.15.0",
    "yup": "^1.1.1"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "axios": "0.25.0"
      },
      "ignoreMissing": [
        "postcss",
        "postcss-*",
        "webpack",
        "axios"
      ]
    }
  },
  "eslintConfig": {
    "extends": "@antfu"
  },
  "release-it": {
    "git": {
      "commit": true,
      "tag": true,
      "push": true,
      "commitMessage": "chore: release v${version}"
    },
    "github": {
      "release": false
    },
    "npm": {
      "publish": false
    }
  }
}
