{
  "name": "app",
  "type": "module",
  "version": "0.7.0",
  "private": false,
  "main": "./nuxt.config.ts",
  "scripts": {
    "dev": "nuxi prepare & nuxi dev --open",
    "build": "nuxi build",
    "serve": "nuxi build && node .output/server/index.mjs",
    "update": "git pull && pnpm update -L",
    "release": "release-it patch --ci",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest --",
    "coverage": "vitest run --coverage",
    "clean": "pnpm recursive exec -- rm -rf node_modules; rm -rf node_modules",
    "fulltest": "pnpm run clean && pnpm config set auto-install-peers true && pnpm install --shamefully-hoist=true && pnpm run lint && pnpm run coverage && pnpm run build"
  },
  "dependencies": {
    "@nuxt/content": "^2.12.0",
    "@nuxt/image": "^1.3.0",
    "@nuxt/kit": "^3.10.2",
    "@nuxt/ui-templates": "^1.3.1",
    "@tato30/vue-pdf": "^1.9.4",
    "@vernaillen/nuxt-base-layer": "0.2.27",
    "nuxt": "^3.10.2",
    "vue-gtag": "^2.0.1"
  },
  "devDependencies": {
    "@nuxt/test-utils": "^3.11.0",
    "@nuxthq/studio": "^1.0.11",
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@nuxtjs/google-fonts": "3.1.3",
    "@nuxtjs/i18n": "^8.1.1",
    "@nuxtjs/plausible": "^0.2.4",
    "@nuxtjs/tailwindcss": "^6.11.4",
    "@pinia/nuxt": "^0.5.1",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@vitest/coverage-v8": "^1.3.0",
    "@vue/test-utils": "^2.4.4",
    "@vueuse/core": "^10.7.2",
    "@vueuse/nuxt": "^10.7.2",
    "animate.css": "^4.1.1",
    "consola": "^3.2.3",
    "cypress": "^13.6.4",
    "dayjs": "^1.11.10",
    "eslint": "^8.56.0",
    "happy-dom": "^13.3.8",
    "nuxt-config-schema": "^0.4.6",
    "nuxt-icon": "^0.6.8",
    "pinia": "^2.1.7",
    "playwright-core": "^1.41.2",
    "remark-directive-rehype": "^0.4.2",
    "sitemap": "^7.1.1",
    "start-server-and-test": "^2.0.3",
    "typescript": "^5.3.3",
    "vee-validate": "^4.12.5",
    "vitest": "1.3.0",
    "vitest-environment-nuxt": "1.0.0",
    "vue-easy-lightbox": "^1.18.0",
    "yup": "^1.3.3"
  }
}
