{
  "name": "app",
  "type": "module",
  "version": "0.6.4",
  "private": false,
  "main": "./nuxt.config.ts",
  "scripts": {
    "dev": "nuxi prepare & nuxi dev --open",
    "build": "nuxi build",
    "serve": "nuxi build && node .output/server/index.mjs",
    "update": "git pull && pnpm update -L",
    "release": "release-it patch --ci",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest --",
    "coverage": "vitest run --coverage",
    "clean": "pnpm recursive exec -- rm -rf node_modules; rm -rf node_modules",
    "fulltest": "pnpm run clean && pnpm config set auto-install-peers true && pnpm install --shamefully-hoist=true && pnpm run lint && pnpm run coverage && pnpm run build"
  },
  "dependencies": {
    "@nuxt/content": "^2.8.2",
    "@nuxt/image": "1.0.0-rc.1",
    "@nuxt/kit": "^3.7.0",
    "@nuxt/ui-templates": "^1.3.1",
    "@vernaillen/nuxt-base-layer": "0.2.18",
    "nuxt": "^3.7.0",
    "vue-gtag": "^2.0.1"
  },
  "devDependencies": {
    "@nuxthq/studio": "^0.13.4",
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@nuxtjs/google-fonts": "3.0.2",
    "@nuxtjs/i18n": "8.0.0-beta.9",
    "@nuxtjs/plausible": "^0.2.1",
    "@nuxtjs/tailwindcss": "^6.8.0",
    "@pinia/nuxt": "^0.4.11",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.6",
    "@tailwindcss/typography": "^0.5.9",
    "@vitest/coverage-c8": "^0.33.0",
    "@vitest/coverage-v8": "^0.34.3",
    "@vueuse/core": "^10.4.1",
    "@vueuse/nuxt": "^10.4.1",
    "animate.css": "^4.1.1",
    "consola": "^3.2.3",
    "cypress": "^13.1.0",
    "dayjs": "^1.11.9",
    "eslint": "^8.48.0",
    "happy-dom": "^10.11.2",
    "nuxt-config-schema": "^0.4.6",
    "nuxt-icon": "^0.5.0",
    "nuxt-vitest": "^0.10.4",
    "pinia": "^2.1.6",
    "remark-directive-rehype": "^0.4.2",
    "sitemap": "^7.1.1",
    "start-server-and-test": "^2.0.0",
    "typescript": "^5.2.2",
    "vee-validate": "^4.11.3",
    "vitest": "0.33.0",
    "vitest-environment-nuxt": "0.10.4",
    "vue-easy-lightbox": "^1.16.0",
    "yup": "^1.2.0"
  }
}
